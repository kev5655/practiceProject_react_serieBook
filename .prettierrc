{
    "plugins": [
        "prettier-plugin-sql"
    ],
    "printWidth": 200,
    "tabWidth": 2,
    "useTabs": false,
    "overrides": [
        {
            "files": [
                "*.sql"
            ],
            "options": {
                "parser": "sql",
                "sqlFormatter": "sql-formatter",
                "printWidth": 200,
                "sqlFormatterOptions": {
                    "language": "mysql",
                    "uppercase": true,
                    "linesBetweenQueries": 1,
                    "tabWidth": 0,
                    "indent": "",
                    "commaPosition": "trailing"
                }
            }
        }
    ]
}